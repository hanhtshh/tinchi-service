{
  "name": "telio-template-service",
  "version": "1.0.5",
  "description": "Buy now pay later",
  "engines": {
    "node": "16.20.0"
  },
  "main": "./src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "make bump-version",
    "postrelease": "git push origin HEAD --tags --no-verify",
    "lint": "eslint ./src/**/*.ts",
    "typecheck": "npx tsc --project tsconfig.json --noEmit --skipLibCheck",
    "migration:create": "sequelize migration:generate --name",
    "migration:status": "sequelize db:migrate:status",
    "migration:up": "sequelize db:migrate",
    "migration:down": "sequelize db:migrate:undo --name",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rimraf ./build && tsc",
    "prestart": "npm run migration:up",
    "start": "node dist/server.js",
    "start:dev": "npm run migration:up & make start-dev",
    "debug": "make debug-ts",
    "prepare": "husky install",
    "kc-migrate-auth": "node ./scripts/keycloak-migration/script.mjs"
  },
  "husky": {
    "hooks": {
      "pre-coomit": "npm run lint & npm run typecheck",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "bnpl"
  ],
  "author": "hienpham",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/moment": "^2.13.0",
    "@types/multer-s3": "^2.7.11",
    "@types/pg": "^8.6.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.5.2"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "rimraf": "^3.0.2",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.26.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "mysql2": "^3.1.0",
    "nocache": "^3.0.1",
    "pg": "^8.7.1",
    "redis": "^4.6.5",
    "sequelize": "^6.12.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0",
    "yarn": "^1.22.17"
  }
}
